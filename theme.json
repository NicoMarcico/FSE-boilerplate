{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "#ffffff"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#e97451"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#51c6e9"
				},
				{
					"name": "Background",
					"slug": "background",
					"color": "#ededed"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "#707070"
				},
				{
					"name": "Foreground darker",
					"slug": "foreground-darker",
					"color": "#2d2d2d"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"customSpacingSize": false,
			"spacingScale": {
				"steps": 0
			},
			"blockGap": true,
			"spacingSizes": [
				{
					"size": "0.625rem",
					"slug": "20",
					"name": "XX-Small"
				},
				{
					"size": "1.25rem",
					"slug": "30",
					"name": "X-Small"
				},
				{
					"size": "1.875rem",
					"slug": "40",
					"name": "Small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.875rem, 4vw, 3.125rem)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(1.875rem, 6vw, 4.375rem)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(3.125rem, 7vw, 5rem)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(4.375rem, 9vw, 8.75rem)",
					"slug": "80"
				}
			]
		},
		"custom": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"open-sans\", sans-serif",
					"name": "Primary",
					"slug": "primary",
					"fontFace": [
						{
							"fontFamily": "open-sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Regular.woff2",
								"file:./assets/fonts/open-sans/OpenSans-Regular.woff",
								"file:./assets/fonts/open-sans/OpenSans-Regular.ttf"
							]
						},
						{
							"fontFamily": "open-sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Bold.woff2",
								"file:./assets/fonts/open-sans/OpenSans-Bold.woff",
								"file:./assets/fonts/open-sans/OpenSans-Bold.ttf"
							]
						},
						{
							"fontFamily": "open-sans",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Italic.woff2",
								"file:./assets/fonts/open-sans/OpenSans-Italic.woff",
								"file:./assets/fonts/open-sans/OpenSans-Italic.ttf"
							]
						},
						{
							"fontFamily": "open-sans",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Bolditalic.woff2",
								"file:./assets/fonts/open-sans/OpenSans-Bolditalic.woff",
								"file:./assets/fonts/open-sans/OpenSans-Bolditalic.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "\"yantramanav\", sans-serif",
					"name": "Secondary",
					"slug": "secondary",
					"fontFace": [
						{
							"fontFamily": "yantramanav",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/yantramanav/Yantramanav-Black.woff2",
								"file:./assets/fonts/yantramanav/Yantramanav-Black.woff",
								"file:./assets/fonts/yantramanav/Yantramanav-Black.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "0.875rem",
					"fluid": false
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.5rem",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					}
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "2rem",
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					}
				},
				{
					"name": "Extra Extra Large",
					"slug": "xx-large",
					"size": "3.5rem",
					"fluid": {
						"min": "3rem",
						"max": "3.5rem"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--block-gap)",
			"padding": {
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.5"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "900",
					"lineHeight": "1"
				},
				"color": {
					"text": "var(--wp--preset--color--foreground-darker)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground-darker)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					}
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontWeight": "700"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
